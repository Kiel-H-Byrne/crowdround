{
	"name": "crowdround",
	"private": true,
	"version": "3.0.0",
	"repository": {
		"url": "https://github.com/tdotholla/crowdround"
	},
	"license": "MIT",
	"eslintConfig": {
		"extends": "react-app"
	},
	"dependencies": {
		"@material-ui/core": "^4.2.1",
		"@material-ui/icons": "^4.2.1",
		"isomorphic-fetch": "^2.2.1",
		"next": "^9.0.0",
		"next-offline": "^4.0.0",
		"react": "16.9.0",
		"react-axe": "^3.2.0",
		"react-dom": "16.9.0"
	},
	"scripts": {
		"dev": "next",
		"start": "next start",
		"lint": "eslint components",
		"build": "next build",
		"build:server": "NEXT_TARGET=server next build",
		"export:server": "NEXT_TARGET=server next export",
		"export": "next export",
		"start:static": "serve",
		"now-build": "next build"
	},
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react",
      "react-hooks"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    }
  },
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"eslint": "^6.2.0",
		"eslint-config-react-app": "^5.0.1",
		"eslint-plugin-flowtype": "^2.50.3",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^1.5.0",
		"serve": "^11.1.0"
	}
}
